apiVersion: v1
kind: Config

clusters:
  - cluster:
    name: development
# Creaci√≥n de usuario 'custom-user' indicando los paths de los certificados
users:
  - name: custom-user
    user:
      client-certificate: /ruta/a/usuario.crt
      client-key: /ruta/a/usuario.key
contexts:
  - name: custom-user-development-access
    context:
      cluster: development
      namespace: dev-namespace
      user: custom-user
