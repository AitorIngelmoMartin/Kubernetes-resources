apiVersion: v1
kind: Config

clusters:
  - cluster:
    name: development
# Creaci√≥n de usuario 'custom-user' indicando los paths de los certificados
users:
  - name: martin
    user:
      client-certificate: /ruta/a/usuario.crt
      client-key: /ruta/a/usuario.key
contexts:
  - name: developer
    context:
      cluster: kubernetes
      user: martin
